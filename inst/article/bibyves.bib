  Doc-delivery-number      = {013CJ},
  Doi                      = {10.1016/j.apmr.2005.09.021},
  ISSN                     = {0003-9993},
  Journal-iso              = {Arch. Phys. Med. Rehabil.},
  Keywords                 = {disabled persons; health care surveys; Medicare; rehabilitation; sampling
 bias; sampling studies},
  Language                 = {English},
  Number-of-cited-references = {9},
