@Article{Rama:Rao:allo:1974,
  Title                    = {Allocation to Strata and Relative Efficiencies of Stratified and Unstratified pPS Sampling Schemes},
  Author                   = {Ramachandran, Geetha and Rao, Talluri Jagannadha},
  Journal                  = {Journal of the Royal Statistical Society},
  Year                     = {1974},
  Number                   = {2},
  Pages                    = {292--298},
  Volume                   = {B36},
  Abstract                 = {The problem of optimum allocation to strata has been earlier examined
 in the light of a priori distributions. In this context, under the
 criterion of minimum expected variance, the sampling strategy consisting
 of an unstratified p PS sampling scheme together with the Horvitz-Thompson
 (HT) estimator was shown to be inferior to the strategy consisting
 of a stratified p PS sampling scheme with the corresponding HT estimator
 with this optimum allocation. In this paper, when stratification
 is based on the auxiliary information, we study whether a stratified
 p PS sampling strategy with various non-optimal allocations is likely
 to be worth while and whether it should be attempted at all. For
 populations commonly met in practice, we derive sufficient conditions
 for unstratified p PS sampling to be preferable to non-optimal stratified
 p PS sampling. An illustrative example is provided towards the end
 of the paper.},
  Address                  = {New York},
  Copyright                = {Copyright Â© 1974 Royal Statistical Society},
  ISSN                     = {00359246},
  Language                 = {English},
