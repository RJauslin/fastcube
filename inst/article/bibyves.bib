@InProceedings{dip:kub:04,
  Title                    = {New approaches for preservation and promotion of dry grasslands in
 the consrvation politics in {S}witzerland},
  Author                   = {Dipner, M. and Madl Kubik, E.},
  Booktitle                = {Land Use Systems in Grassland Dominated Regions},
  Year                     = {2004},
  Editor                   = {L\"uscher, A. and Jeangros, B. and Kessler, W. and Huguenin, O. and Lobsiger, M. and Millar, N. and Suter, D.},
  Pages                    = {85--87},
  Publisher                = {{S}wiss Grassland Society (AGFF)},
  Series                   = {Grassland Science in Europe},
  Volume                   = {9},
  Abstract                 = {The inventory of the increasingly rare {S}wiss dry grasslands which
 are rich in species will soon be completed. For nature and landscape
 conservation purposes, an area of more than 20,000 ha will be designated
 as sites of national importance (which is about 1-2 % of the entire
 agricultural land of Switzerland). These grassland sites are the
 evidence and the outcome of a traditional, regionally adapted land use of low intensity. They are elements of a traditional agrarian
 landscape and are hence important for tourism. The remaining relicts
 are mainly endangered by abandonment. Up-to-date methods (interpretation
 of aerial photographs, GPS-calibrated species data sampling) have
 been adopted for survey and evaluation of the sites of national importance.
 A specific legal foundation is the basis for both land use subsidies
 and conservation measures. The national conservation strategy will
 be attached to the different models for implementing regional conservation.
 It should provide an incentive. Subsidy contracts will be offered
 to the farmers to keep them using grasslands with low yield but high
 bio-diversity. They should be provided with sustainable alternatives
 to abandonment. In a new approach the conservation strategy aims
 to create `priority zones' and to strengthen the co-operation with
 the agrarian bodies. Is it an optimal approach of ours to enlist
 our partners by means of agreements?},
  Keywords                 = {TWW, bio-diversity, habitat conservation, low intense land use, inventory,
 dry grasslands, conservation politics}
}

